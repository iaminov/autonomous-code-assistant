# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4-turbo-preview

# Project Configuration
PROJECT_ROOT=.
LOG_LEVEL=INFO
MAX_TOKENS=4096
TEMPERATURE=0.2

# Test Configuration
RUN_TESTS_AFTER_CHANGES=true
TEST_TIMEOUT=300

# File Processing
IGNORE_PATTERNS=*.pyc,__pycache__,*.git*,.env*,*.log
MAX_FILE_SIZE_MB=10
